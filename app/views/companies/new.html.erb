<h1>Create a new company</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@company) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :email %>
      <%= f.text_field :email %>
      <div class="more">
     <div id="container">
             <div style="width:400px;"><%= f.submit "Save changes", class: "btn btn-primary" %></div>
           <% end %>
             <div style="width:100px;"><%= link_to "Back", companies_path, class: "btn btn-primary"  %></div>
         </div>
       </div>
  </div>
</div>
